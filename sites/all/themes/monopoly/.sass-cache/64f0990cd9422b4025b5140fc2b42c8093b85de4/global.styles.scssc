}

/*
 * Define consistent borders
 */
fieldset {
  border: 1px solid $border;
}

/*
 * Tips for Drupal's input formats
 */
.tips { 
}


/*
 * Buttons used by contrib modules like Media
 */
a.button { 
  @include appearance(button);
}


/*
 * Password confirmation
 */
.password-parent,
.confirm-parent {
  margin: 0;
}


/* =============================================================================
 *   Tables
 * ========================================================================== */

table {
  margin: 10px 0;
  padding: 0;
  width: 100%;

  &.sticky-header {
    z-index: 10;
  }
}


table,
thead,
tbody,
tr,
th,
td {
  border-color: $border;
}


table,
td,
th {
  vertical-align: middle;
}


caption,
th,
td {
  text-align: left;
}


thead tr {
  font-weight: 700;
  background-color: $medium_dark;
}


td,
th {
  border-bottom: 0;
  margin: 0;
  padding: 5px 7px;
}


tbody {}


tbody tr {
  border-top: 1px solid $border;
}


tr {

  &.odd {
    background: $page;
  }

  /* Table row striping */
  &.info,
  &.even,
  &:nth-child(2n+2)  {
    border-bottom: 0;
    background-color: $ultralight;
  }

  &.drag {}

  &.drag-previous {}

  &.odd td.active {
    background-color: $light;
  }

  &.even td.active {
    background-color: $medium_light;
  }
}


.lt-ie8 tr {

  &.even,
  &.odd {

    th,
    td {
    }
  }
}


/*
 * Forum tables
 * Core sets white-space to nowrap, which makes no sense
 */
#forum td {
  &.created,
  &.posts,
  &.topics,
  &.last-reply,
  &.replies,
  &.pager {
    white-space: normal;
  }
}


/* =============================================================================
 *   Messages
 * ========================================================================== */

div {
  &.messages {
  // Left and right margin are set by the global gutter width
  margin-bottom: 10px;
  margin-top: 10px;

    ul {
      margin-top: 0;
      margin-bottom: 0;
    }
  }

  &.status {
  }

  &.warning {
  }
}

tr.warning {
}


div.error,
tr.error {
}


.error {
}


.warning {
}


/*
 * Unpublished nodes
 */
.node-unpublished,
.comment-unpublished {
}


.node-unpublished,
.comment-unpublished {

  p.unpublished {
    @include unpublished;
  }
}


/* =============================================================================
 *    Maintenance pages
 * ========================================================================== */

.maintenance-page {
  .container {
    padding: 40px 0;
  }

  #site-name,
  #page-title {
    margin: 0;
  }
}


.db-offline {
  .container {
    margin: 0 auto;
    padding: 40px 0;
    width: 100%;
    max-width: 960px;
  }

  div.messages {
    margin: 20px 0 0;
  }

  #content {
    padding: 20px 0;
  }
}


/* =============================================================================
 *   Misc overrides for contrib modules
 * ========================================================================== */

/*
 * Admin menu overrides
 */
#admin-menu {
  margin: 0;
  padding: 0;
}


/*
 * Devel Module
 */
.dev-query {
  background: $light;
  padding: 30px;
}


/*
 * Styleguide module tweaks
 */
#styleguide-header {
  padding: 0 10px;

  .item-list {
    font-family: inherit;
    margin: 0 20px 20px 0;
    min-height: 260px;
    width: auto;
  }
}


html {
  background-color: #030303;
  color: white;
}

a {
  color: #E0C27C;
}

h1 {
  margin-top: 0;
  margin-left: 10px;
  font-size: 2.5em;
}

h1.field-content {
  margin-left: 0;
  margin-bottom: 0;
}

h2 {
  font-size: 2em;
  line-height: 1em;
  margin-top: 0;
}

h3 {
  margin-bottom: .5em;
}

h3.field-name-field-title-sponsor {
  margin: 0 0 0 10px;
  text-transform: uppercase;
  padding: 0;
  font-size: 1em;
}

#site-name {
  font-weight: 400!important;
  font-size: 25px;
}

#site-name a:link, #site-name a:visited {
  color: #FC3902;
}

#site-name a:hover {
  text-decoration: none;
}

a span.logo-secondpart {
  color: white;
  letter-spacing: 4px;
}

#header {
  float: left;
  width: auto;
}

#primary-menu-bar {
  float: right;
  width: auto;
  clear: none;
  margin-top: 5px;
}

#page {
  padding-top: 15px;
  position: relative;
}

#page:before {
  content: url(../../../../all/themes/monopoly/images/reese.jpg);
  position: absolute;
  width: 928px;
  left: 0;
  height: 1584px;
  margin-left: -200px;
  margin-top: 30px;
  z-index: -10;
}

.section-productions #page:before, .section-performances  #page:before, .node-type-performance #page:before {
  content: "";
}

.page-performances #page:before {
  content: "";
}

.page-contact #page:before {
  content: url(../../../../all/themes/monopoly/images/reese-contact.jpg);
  width: 739px;
  left: 0;
  height: 545px;
  margin-left: -40px;
  margin-top: 180px;
}

.section-blog #page:before {
  content: url(../../../../all/themes/monopoly/images/reese-bg.jpg);
  width: 494px;
  height: 708px;
  margin-left: -100px;
}

div.messages {
  clear: both;
}

.front .region-two-33-66-second {
  float: right;
}

.front .views_slideshow_main {
  border: solid 12px #222222;
  margin-top: 20px;
  background-color: #222;
}
.front .region-two-33-66-second {
	float: right!important;
}

.section-productions .views_slideshow_main {
  margin-bottom: 30px;
}

.views_slideshow_controls_text_pause {
  display: none;
}

.views_slideshow_controls_text_previous {
  position: absolute;
  top: 45%;
  left: -10px;
  z-index: 101;
  text-indent: 100%;
  white-space: nowrap;
  overflow: hidden;
  width: 31px;
  height: 61px;
  background: url(../../../../all/themes/monopoly/images/left-arrow.png) no-repeat center center;
}

.views_slideshow_controls_text_next {
  position: absolute;
  top: 45%;
  right: -10px;
  z-index: 101;
  text-indent: 100%;
  white-space: nowrap;
  overflow: hidden;
  width: 31px;
  height: 61px;
  background: url(../../../../all/themes/monopoly/images/right-arrow.png) no-repeat center center;
}

.view-id-slideshow {
  position: relative;
}

h2.pane-title, h2.block-title {
  text-transform: lowercase;
  color: #FC3902;
  font-weight: 400!important;
  font-size: 17px;
  letter-spacing: 1px;
  margin: 10px 0 12px;
}

input[type='text'] {
  max-width: 95%;
  display: block;
  margin-bottom: 3px;
  padding-top: 20px;
  padding-bottom: 20px;
  text-indent: 15px;
  font-size: 14px;
  background-color: #262F31;
  color: #B1B0B0;
  border: none;
  outline: none;
}

.form-type-textfield, .form-type-textarea {
  width: 100%;
}

#content-column {
  margin-top: 40px;
}

.view-productions .view-content {
	text-align: justify;
	.views-row {
		width: 100%;
		display: inline-block;
		vertical-align: top;
		text-align: left;
		position: relative;
		width: 31%;
		}
	
	&:after {
		content: "";
		display: inline-block;
		width: 100%;
	}
}


.views-fluidgrid-item-inner {
  width: 260px;
  margin-bottom: 15px;
}

.views-fluidgrid-item-inner .ds-1col {
  background-color: #ffffff;
  line-height: 13px;
  padding-top: 15px;
  border-right-width: 0px;
  padding-left: 15px;
  padding-right: 15px;
}

.group-blog-main-image {
  float: left;
  margin-right: 10px;
}

.label-inline {
  float: left;
  color: silver;
  text-transform: uppercase;
}

.label-above {
  color: silver;
  text-transform: uppercase;
}

h2 {
  margin-bottom: 5px;
}

.field-name-post-date {
  color: silver;
  letter-spacing: 1px;
  text-transform: uppercase;
  font-size: .8em;
  margin-bottom: 10px;
}

.node-teaser {
  clear: left;
  margin-bottom: 10px;
  padding-bottom: 5px;
  border-bottom: 1px dotted silver;
}

.field-name-field-teaser-summary {
  margin-top: 8px;
}

textarea {
  display: block;
  width: 90%;
  height: 140px;
  margin-bottom: 11px;
  padding-top: 20px;
  font-size: 14px;
  line-height: 14px;
  text-indent: 15px;
  background-color: #262F31;
  color: #B1B0B0;
  overflow: auto;
  border: none;
  outline: none;
}

.form-type label {
  margin-left: 5px;
  font-size: 14px;
  line-height: 28px;
  color: #8B999D;
}

.form-type-textfield label, .form-type-textarea label {
  display: block;
}

.form-submit {
  display: inline-block;
  height: 55px;
  margin-top: 10px;
  font-size: 14px;
  line-height: 55px;
  text-decoration: none;
  background-color: #FC3902;
  text-transform: uppercase;
  font-family: 'pragmatica-web-condensed',sans-serif;
  font-style: normal;
  color: white;
  border: none;
  outline: none;
  cursor: pointer;
  padding: 0 15px;
}

.form-submit:hover {
  background-color: #B61B02;
}

.grippie {
  display: none;
}

.view-monthly-archive ul, .view-tags ul {
  list-style: none;
  padding-left: 0;
}

.view-monthly-archive ul li, .view-tags ul li {
  padding-left: 0;
  margin-left: 0;
}

.field-name-field-production-main-image {
  float: left;
  margin: 10px;
}

.read-more a {
  padding: 5px 10px;
  font-size: .9em;
  display: inline-block;
  background-color: #222;
  text-transform: uppercase;
  font-family: 'pragmatica-web-condensed',sans-serif;
  font-style: normal;
  border: none;
  outline: none;
  cursor: pointer;
  color: white;
  text-decoration: none;
}

.read-more a:hover {
  background-color: #FC3902;
  color: white;
}

.quote {
  font-size: 2.2em;
}

.quote p {
  margin-bottom: .5em;
}

.attribution {
  font-family: 'pragmatica-web-condensed',sans-serif;
  font-style: normal;
  color: white;
  font-size: 1em;
  margin-left: 0;
  margin-bottom: 1em;
}

.list {
  padding-bottom: 10px;
  margin-bottom: 10px;
  text-indent: 20px;
}

.list.views-row-last {
  margin-bottom: 20px;
  padding-bottom: 20px;
  border-bottom: solid 5px #222222;
}

.view-blog .views-row {
  margin-bottom: 40px;
}

.field-name-field-production-artist {
  float: left;
  clear: left;
}

#toolbar .toolbar-shortcuts ul li {
  font: normal "Lucida Grande", Verdana, sans-serif!important;
  font-size: 1em!important;
}

.view-tags ul li {
  float: left;
  padding: 3px 5px;
  margin: 5px 2px;
  border: solid 1px gray;
}

a.menu-1611, a.menu-1612 {
  margin-top: -5px;
  width: 20px;
  height: 32px;
}

.service-links ul {
  padding: 0;
}

.service-links ul li {
  list-style: none;
  margin: 0;
  float: left;
}

.inline-block {
  display: inline;
  margin-right: 4px;
}

.field-name-field-tags {
  display: inline-block;
}

.label-inline {
  margin-right: 4px;
}

.field-name-field-reviews-body .label-inline {
  color: white;
  text-transform: lowercase;
}

.pager {
  margin-top: 25px;
  background-color: #222;
  padding: 25px 10px 18px;
}

.freepager-next {
  position: relative;
  top: -25px;
}

.freepager-previous {
  position: relative;
  top: -25px;
}

.freepager-next::after {
  content: "";
  display: inline-block;
  background: url(../../../../all/themes/monopoly/images/right-arrow.png) no-repeat right center;
  width: 31px;
  height: 61px;
  position: relative;
  top: 25px;
}

.freepager-previous::before {
  content: "";
  display: inline-block;
  width: 31px;
  height: 61px;
  background: url(../../../../all/themes/monopoly/images/left-arrow.png) no-repeat left center;
  position: relative;
  top: 25px;
}

.section-performances #page-title, .node-type-performance #page-title {
  display: none;
}

.page-performances #page-title {
  display: block;
}

.pane-node-field-date h2 {
  margin-top: 0;
}

.pane-performance-backlinks-panel-pane-1 {
  margin-bottom: 0;
}

.push-down {
  margin-bottom: 30px;
}

@media (max-width: 480px) {
  #admin-menu {
    display: none;
  }

  .contextual-links-region {
    position: static;
  }

  #primary-menu-bar {
    float: left;
    width: auto;
    clear: none;
    margin-top: 5px;
    position: absolute;
    z-index: 300;
    background-color: black;
  }

  .logged-in #branding {
    margin-top: -40px;
  }
}

